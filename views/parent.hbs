{{/if}}

<!-- Profile area -->

<div class="emp-profile">
    <form class="form-container" method="post">
        <div class="row">
            <div class="col-md-3">
                <div class="profile-img">
                    <img src="{{parent.image}}" alt="" />
    
                    {{#if family}}
                    <div class="file btn btn-lg btn-primary">
                        {{!-- Change Photo --}}
                       <a href="/parent/{{parent.id}}/edit">  Change Photo</a>
                    </div>
                    {{/if}}
                </div>
            </div>
            <div class="col-md-6">
                <div class="profile-head">
                    <p class="profile-rating">RANKINGS : <span>8/10</span></p>
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab"
                                aria-controls="home" aria-selected="true">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="profile-tab" data-toggle="tab" 
                            href="#profile" role="tab" aria-controls="profile" aria-selected="true">Extra Info</a>
                        </li>

                    <li class="nav-item">
                    {{#if family}}
                        <div>
                    
                            <button class="btn btn-default"> <a href="/parent/{{parent.id}}/edit">Settings</a></button>
                        </div>
                     {{/if}}
                    </li>
                    </ul>
                </div>
            </div>
  
      
        </div>

        <div class="row under-pic">
            <div class="col-md-8">
                <div class="tab-content profile-tab" id="myTabContent">
                    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                        <div class="row">
                            <div class="col-md-6">
                                <label>Name</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.firstName}} {{parent.lastName}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Username</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.username}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Phone</label>
                            </div>
                            <div class="col-md-6">
                                <p>123 456 7890</p>
                            </div>
                        </div>

                           <div class="row">
                            <div class="col-md-6">
                                <label>Role</label>
                            </div>
                            <div class="col-md-6">
                                <p>Parent</p>
                            </div>
                        </div>

                    </div>

                    <div class="tab-pane fade show active" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                        <div class="row">
                            <div class="col-md-6">
                                <label>Country</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.country}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>City</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.city}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Kids</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.kids}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label>Days</label>
                            </div>
                            <div class="col-md-6">
                                <p>{{parent.days}}</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <label>Joined at</label><br />
                                <p>{{parent.createdAt}}</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

<footer class="footer">
    <p>Copyright &copy; {{title}} <script>document.write(new Date().getFullYear())</script></p>
</footer>
